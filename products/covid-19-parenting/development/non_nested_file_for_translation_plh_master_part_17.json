  },
  "334": {
    "has_extraline": 0,
    "flow_id": "409a392c-6dca-40bf-8a5a-edd3aa53d71e",
    "bit_id": "c0af0043-637e-4580-b1f2-508c007522b9",
    "bit_type": "text",
    "type_id": 3,
    "text": "After ten seconds, how many fingers do you have up? What sounds did you hear?",
    "has_bullet": true
  },
  "349": {
    "has_extraline": 0,
    "flow_id": "409a392c-6dca-40bf-8a5a-edd3aa53d71e",
    "bit_id": "c0af0043-637e-4580-b1f2-508c007522b9",
    "bit_type": "text",
    "type_id": 4,
    "text": "Try again for another ten seconds. ",
    "has_bullet": true
  },
  "356": {
    "has_extraline": 0,
    "flow_id": "409a392c-6dca-40bf-8a5a-edd3aa53d71e",
    "bit_id": "c0af0043-637e-4580-b1f2-508c007522b9",
    "bit_type": "text",
    "type_id": 5,
    "text": "Can you hear any new sounds? Can you hear any sounds inside your own body?",
